{
    "name": "tuhin-su/livewire-swal",
    "description": "SweetAlert2 integration for Laravel Livewire with easy-to-use methods and event handling",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "livewire", 
        "sweetalert2",
        "alert",
        "notification",
        "confirmation"
    ],
    "authors": [
        {
            "name": "Tuhin Su",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LivewireSwal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LivewireSwal\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LivewireSwal\\LivewireSwalServiceProvider"
            ],
            "aliases": {
                "Swal": "LivewireSwal\\Facades\\Swal"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
